{
  "name": "marmokbot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec \"node --no-warnings --loader ts-node/esm src/main.ts\"",
    "start": "node build/main.js",
    "watch": "nodemon -e ts --watch src --exec npm run dev",
    "migrate": "ts-node ./src/migrations/migrate.ts"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/pagination": "^3.5.5",
    "discord.js": "^14.17.3",
    "discordx": "^11.12.2",
    "dotenv": "^16.4.7",
    "mysql2": "^3.14.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^22.14.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
